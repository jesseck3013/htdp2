; Nelon -> Number
; determines the smallest number on l
(define (sup.v2 l)
  (cond
    [(empty? (rest l)) (first l)]
    [else
     (local ((define biggest-in-rest (sup.v2 (rest l))))
       (if (> (first l) biggest-in-rest)
           (first l)
           biggest-in-rest))]))

(sup.v2 (list 2 1 3))
